    <container name="listing_top">
        <massaction name="listing_massaction">
            <action name="eboekhouden_export_order">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">eboekhouden_export_order</item>
                        <item name="label" xsi:type="string" translate="true">Export naar e-Boekhouden.nl</item>
                        <item name="url" xsi:type="url" path="eboekhouden_export/sales/invoice">
                    </item>
                </item></argument>
            </action>
        </massaction>
    </container>
    <columns name="sales_order_invoice_columns">
        <column name="eboekhouden_mutatie">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="filter" xsi:type="string">text</item>
                    <item name="sorting" xsi:type="string">desc</item>
                    <item name="label" xsi:type="string" translate="true">Mutatie nummer</item>
                    <item name="sortOrder" xsi:type="number">75</item>
                </item>
            </argument>
        </column>
    </columns>
</listing>
