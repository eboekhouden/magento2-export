<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<container name="listing_top">
        <massaction name="listing_massaction">
            <action name="eboekhouden_export_order">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">eboekhouden_export_order</item>
                        <item name="label" xsi:type="string" translate="true">Export naar e-Boekhouden.nl</item>
                        <item name="url" xsi:type="url" path="eboekhouden_export/sales/order">
                    </item>
                </item></argument>
            </action>
        </massaction>
    </container>
</listing>
